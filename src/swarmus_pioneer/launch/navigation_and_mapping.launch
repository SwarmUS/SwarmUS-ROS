<?xml version="1.0" ?>
<launch>
    <arg name="robot_name" default="pioneer_0"/>

    <group ns="$(arg robot_name)">
        <!-- Navigation stack -->
        <include file="$(find swarmus_ros_navigation)/launch/navigation.launch">
            <arg name="robot_name" value="$(arg robot_name)"/>
        </include>
    </group>
</launch>
